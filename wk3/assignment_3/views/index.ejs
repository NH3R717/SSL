<body id="page-top">
    <!-- ejs Navigation -->

    <%- include('partial/nav')  %>

    <!-- Masthead-->
    <header class="masthead bg-primary text-white text-center">

        <div style="background-color: #1abc9c;">
            <%if(typeof(errors) != "undefined"){%>
            <% for(e in errors){%>
            <br><span style="color: #f50000">Login Error:<%= errors[e]%></span>
            <%}%>
        <%}%>
        </div>
        <div style="background-color: #1abc9c; padding: 1em 0 4em 0;">
            <%if(typeof(success) != "undefined"){%>
            <br><span style="color: #006a00">Login Success:</span>
            <% for(s in success){%>
            <br><span><%= success[s]%></span>
            <%}%>
        <%}%>
        </div>

        <div class="container d-flex align-items-center flex-column">
            <!-- Masthead Avatar Image-->
            <!-- Masthead Heading-->
            <h1 class="masthead-heading text-uppercase mb-0">SSL / Assignment 3</h1>
            <!-- Icon Divider-->
            <div class="divider-custom divider-light">
                <div class="divider-custom-line"></div>
                <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
                <div class="divider-custom-line"></div>
            </div>
            <!-- Masthead Subheading-->
            <p class="masthead-subheading font-weight-light mb-0">Tommy Umholtz – Full Sail – Web Design/Development</p>
        </div>
    </header>

    <!-- ABOVE NOT ALTER -->

    <!-- About Section-->
    <section class="page-section bg-primary text-white mb-0" id="about">
